<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="InchooDev\TicketManager\Core\Content\Ticket\TicketDefinition">
            <tag name="shopware.entity.definition" entity="inchoo_ticket" />
        </service>

        <service id="InchooDev\TicketManager\Core\Content\TicketMessage\TicketMessageDefinition">
            <tag name="shopware.entity.definition" entity="inchoo_ticket_message" />
        </service>
    </services>
</container>